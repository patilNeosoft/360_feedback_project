                        <li class="nav-item nav-profile dropdown">
                            <a class="nav-link dropdown-toggle" href="#" data-bs-toggle="dropdown" id="profileDropdown">
                                <img src="~/lib/Assets/images/faces/user.png" alt="profile" />
                            </a>
                            <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="profileDropdown">
                                <h6 id="usernameHeader">@TempData["Username"]</h6>
                                <a class="dropdown-item" asp-controller="UserQuery" asp-action="UserQueries">
                                    @*<i class="ti-power-off text-primary"></i>*@
                                    Query/Suggestion
                                </a>
                                <a class="dropdown-item" asp-controller="User" asp-action="ChangePassword">
                                    @*<i class="ti-power-off text-primary"></i>*@
                                    Change Password
                                </a>
                                <a class="dropdown-item" asp-controller="User" asp-action="LogoutUser">
                                    @*<i class="ti-power-off text-primary"></i>*@
                                    <i class="bi bi-escape"></i>
                                    Logout
                                </a>
                              
